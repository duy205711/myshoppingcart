#jssor_1
  / Loading Screen
  .loading{"data-u" => "loading"}
    %div{:style => "filter: alpha(opacity=70); opacity: 0.7; position: absolute; display: block; top: 0px; left: 0px; width: 100%; height: 100%;"}
    .div-loadding
  .slides{"data-u" => "slides"}
    - product = Category.get_comic_book.each do |f|
      %div
        = image_tag(f.picture.url,size: '50x50', data:{ u: 'image' })
        %div{"data-u" => "thumb"}
          = image_tag(f.picture.url, class: 'i')
          .t
          .c= f.name
  / Thumbnail Navigator
  .jssort11{"data-autocenter" => "2", "data-u" => "thumbnavigator"}
    / Thumbnail Item Skin Begin
    %div{"data-u" => "slides", :style => "cursor: default;"}
      .p{"data-u" => "prototype"}
        .tp{"data-u" => "thumbnailtemplate"}
  %span.jssora02l{"data-autocenter" => "2", "data-u" => "arrowleft"}
  %span.jssora02r{"data-autocenter" => "2", "data-u" => "arrowright"}
/ #endregion Jssor Slider End

:javascript
  jssor_1_slider_init();
